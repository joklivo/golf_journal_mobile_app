		holeResultGR.addQuery('play_result', input.playResultID);
		holeResultGR.addQuery('player.system_user', gs.getUserID());
		holeResultGR.query();
	
	while (holeResultGR.next()) {
		holeResultGR.setValue('state', 'completed');
		holeResultGR.update();
	}
 
})(input);]]></execution_script>
